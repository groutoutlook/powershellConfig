            {
                "action": "swapPane",
                "direction": "previous"
            },
            "keys": "win+ctrl+alt+z"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "first"
            },
            "keys": "ctrl+shift+insert"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "first"
            },
            "keys": "ctrl+alt+shift+1"
        },
